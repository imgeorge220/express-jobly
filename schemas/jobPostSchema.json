{
  "definitions": {},
  "$schema": "",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "default": null,
  "additionalProperties": false,
  "required": [
    "title",
    "salary",
    "equity"
  ],
  "properties": {
    "title": {
      "$id": "#/properties/title",
      "type": "string",
      "title": "The Title Schema",
      "default": "",
      "examples": [
        "Worker"
      ],
      "pattern": "^(.*)$"
    },
    "salary": {
      "$id": "#/properties/salary",
      "type": "number",
      "title": "The Salary Schema",
      "default": 0,
      "examples": [
        100
      ],
      "minimum": 0.0
    },
    "equity": {
      "$id": "#/properties/equity",
      "type": "number",
      "title": "The Equity Schema",
      "default": 0.0,
      "examples": [
        0.01
      ],
      "minimum": 0.0,
      "maximum": 1.0
    },
    "company_handle": {
      "$id": "#/properties/company_handle",
      "type": "string",
      "title": "The Company_handle Schema",
      "default": "",
      "examples": [
        "QQQ"
      ],
      "pattern": "^(.*)$"
    }
  }
}